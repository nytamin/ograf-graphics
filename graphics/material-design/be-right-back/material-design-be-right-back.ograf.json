{
  "$schema": "https://ograf.ebu.io/v1/specification/json-schemas/graphics/schema.json",
  "id": "be-right-back-material-design",
  "version": "1.0.0",
  "name": "Material Design Be Right Back",
  "description": "A be-right-back slate with schedule and social links.",
  "author": {
    "name": "OGraf Templates"
  },
  "main": "be-right-back.js",
  "supportsRealTime": true,
  "supportsNonRealTime": false,
  "stepCount": 1,
  "schema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "title": {
        "type": "string",
        "title": "Title",
        "default": "Be Right Back!"
      },
      "subtitle": {
        "type": "string",
        "title": "Subtitle",
        "default": "Fetching more pup content..."
      },
      "scheduleTitle": {
        "type": "string",
        "title": "Schedule Title",
        "default": "What's Next?"
      },
      "scheduleItems": {
        "type": "array",
        "title": "Schedule Items",
        "items": {
          "type": "string"
        },
        "default": [
          "Puppy Training Fails",
          "Skateboard Tricks",
          "Q&A with Johan"
        ]
      },
      "socials": {
        "type": "array",
        "title": "Social Handles",
        "items": {
          "type": "string"
        },
        "default": [
          "@DogNews",
          "discord.gg/dognews",
          "twitch.tv/johan_nyman"
        ]
      },
      "primaryColor": {
        "type": "string",
        "title": "Primary Color",
        "default": "#ff8c00",
        "gddType": "color-rrggbb",
        "pattern": "^#[0-9a-f]{6}$"
      },
      "accentColor": {
        "type": "string",
        "title": "Accent Color",
        "default": "#1a1a1a",
        "gddType": "color-rrggbb",
        "pattern": "^#[0-9a-f]{6}$"
      }
    }
  }
}
